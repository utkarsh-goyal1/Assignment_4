import java.util.*;
public class Main {
    public static void main (String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int []A=new int[n];
        for(int i=0;i<n;i++)
        {
            A[i]=sc.nextInt();
        }
        Arrays.sort(A);
        int s = 0;
        for(int i=0;i<n-1;i=i+2){
            s+=Math.min(A[i],A[i+1]);
        }
        System.out.println(s);
    }
}
